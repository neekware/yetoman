{
  "name": "yetoman",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/yetoman",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/yetoman",
        "outputPath": "dist/apps/yetoman",
        "postcssConfig": "apps/yetoman/postcss.config.js"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/yetoman"
        },
        "production": {}
      },
      "dependsOn": ["build-custom-server"]
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "yetoman:build",
        "dev": true,
        "customServerTarget": "yetoman:serve-custom-server"
      },
      "configurations": {
        "development": {
          "buildTarget": "yetoman:build:development",
          "dev": true,
          "customServerTarget": "yetoman:serve-custom-server:development"
        },
        "production": {
          "buildTarget": "yetoman:build:production",
          "dev": false,
          "customServerTarget": "yetoman:serve-custom-server:production"
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "yetoman:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/yetoman/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/yetoman/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build-custom-server": {
      "executor": "@nrwl/js:tsc",
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/yetoman",
        "main": "apps/yetoman/server/main.ts",
        "tsConfig": "apps/yetoman/tsconfig.server.json",
        "clean": false,
        "assets": []
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve-custom-server": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "yetoman:build-custom-server"
      },
      "configurations": {
        "development": {
          "buildTarget": "yetoman:build-custom-server:development"
        },
        "production": {
          "buildTarget": "yetoman:build-custom-server:production"
        }
      }
    }
  },
  "tags": []
}
