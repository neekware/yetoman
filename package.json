{
  "name": "yetoman",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "<----- Basic Defaults ----->": "",
    "start:prod": "NODE_ENV=production node ./dist/apps/yetoman/server/main.js",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint",
    "<----- Prisma Defaults ----->": "",
    "prisma": "node ./node_modules/.bin/prisma ",
    "prisma:create": "yarn prisma migrate dev --create-only --schema=apps/yetoman/schema/schema.prisma",
    "prisma:apply": "yarn prisma migrate deploy --schema=apps/yetoman/schema/schema.prisma",
    "prisma:dev": "yarn prisma migrate dev --schema=apps/yetoman/schema/schema.prisma",
    "prisma:gen": "yarn prisma generate --schema=apps/yetoman/schema/schema.prisma",
    "prisma:studio": "yarn prisma studio --schema=apps/yetoman/schema/schema.prisma",
    "prisma:seed": "yarn prisma db seed",
    "<----- GraphQL Defaults ----->": "",
    "gql:gen": "graphql-codegen",
    "<----- Utils Related ----->": "Remove --transpileOnly to see errors",
    "loc": "yarn ts-node  --transpileOnly --logError ./tools/utils/loc.ts",
    "lic": "yarn ts-node  --transpileOnly --logError ./tools/utils/lic.ts"
  },
  "prisma": {
    "seed": "ts-node apps/yetoman/schema/seed.ts"
  },
